            <Description>hides the menu</Description>
            <Type>boolean</Type>
            <Default>false</Default>
            <Required>1</Required>
            <EasyMode>
               <Default>false</Default>
               <DataType>boolean</DataType>
               <Constraints>none</Constraints>
            </EasyMode>
         </Arg>
